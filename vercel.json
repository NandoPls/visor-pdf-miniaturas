{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/books/(.*)",
        "dest": "/books/$1"
      },
      {
        "src": "/pdf(.*)",
        "dest": "/pdf$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  